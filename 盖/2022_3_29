#include <stdio.h>
void main()
{
	int a[6][6] = {0}, i, j;
	for (i = 1; i < 6; i++)
		for (j = 1; j < 6; j++)
			a[i][j] = (i / j) * (j / i);
		for (i = 1; i < 6; i++)
		{
			for (j = 1; j < 6; j++)
				printf("%2d", a[i][j]);
			printf("\n");
		}
}
_________________________
#include <stdio.h>
void main()
{
	int i;
	int a[6];
	//数组不能先定义再赋值，要定义的同时赋值;
	for (i = 1; i < 6; i++)
	{
		a[i] = 9 * (i - 2 + 4 * (i > 3)) % 5;
		printf("%2d", a[i]);
	}
}
____________________
//有20阶阶梯，有两种走法:一是一步迈一个阶梯;二是一步迈两个阶梯，问走这20阶梯共有多少种走法？
//一阶楼梯只有1种走法，两阶楼梯有两种走法，三阶楼梯有3种走法，四阶楼梯有5种走法;
//可见第三阶楼梯的走法等于第一阶楼梯走法加第二阶走法;
//设第18阶走法为m,设第19阶走法为n,那么第20阶走法为（m+n);
#include <stdio.h>
int fun(int n)
{
	if (n == 1)
		return 1;
	if (n == 2)
		return 2;
	else
		n =fun(n - 1)+fun(n-2);
	return n;
}
void main()
{
	int i = 20;
	printf("%d\n",fun(i));
}
_________________________
#include <stdio.h>
int fun(int m)
{
	if (m == 1)
		return m;
	else
		m = m * fun(m - 1);
	return m;
}
int main()
{
	int m, n,C;
	m = 0;
	n = 0;
	C = 0;
	printf("请输入m,n，m要大于n:>\n");
	scanf("%d%d", &m, &n);
	C = fun(m) / ((fun(n)) * fun(m - n));
	//C4 2是6;
	printf("%d\n", C);
	return 0;
}
